  </Style>
  <Style name='Table:odd'>
		<StyleAttribute name='backgroundColor'           value='#646464'/>
  </Style>
  <Style name='.tabtitl'>
    <StyleAttribute name='fontSize'                  value='14pt'/>
    <StyleAttribute name='fontStyle'                 value='italic'/>
    <StyleAttribute name='textColor'                 value='blue'/>
    <StyleAttribute name='textDecoration'            value='underline'/>
  </Style>

  <Style name='Window'>
    <StyleAttribute name='fontSize'                  value='10pt'/>
    <StyleAttribute name='ringMenuPosition'          value='right'/>
    <StyleAttribute name='actionPanelPosition'       value='right'/>
    <StyleAttribute name='statusBarType'             value='panels2'/>
    <StyleAttribute name='startMenuSize'             value='small' />
    <StyleAttribute name='startMenuPosition'         value='tree' />
    <StyleAttribute name="backgroundColor"      value="#121219" />
		<StyleAttribute name="backgroundGradientType" value="linear|top|bottom" />
		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
    <StyleAttribute name='textColor' value='darkBlue'/>
  </Style>
  <Style name='Window.prompt'>
    <StyleAttribute name='toolBarPosition'          value='none'/>
