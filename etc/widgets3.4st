		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
    <StyleAttribute name='textColor' value='darkBlue'/>
  </Style>
  <Style name='TopMenu'>
    <StyleAttribute name="backgroundColor"      value="#121219" />
    <StyleAttribute name='textColor' value='darkBlue'/>
  </Style>
  <Style name='ToolBar'>
    <StyleAttribute name="backgroundColor"      value="#121219" />
		<StyleAttribute name="backgroundGradientType" value="linear|top|bottom" />
		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
    <StyleAttribute name='textColor' value='lightCyan'/>
  </Style>
  <Style name='VBox'>
    <StyleAttribute name="backgroundColor"      value="#121219" />
		<StyleAttribute name="backgroundGradientType" value="linear|top|bottom" />
		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
  </Style>
  <!-- Style name='HBox'>
    <StyleAttribute name="backgroundColor"      value="#121219" />
		<StyleAttribute name="backgroundGradientType" value="linear|top|bottom" />
		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
  </Style-->
  <Style name='Folder'>
    <StyleAttribute name="backgroundColor"      value="#121219" />
		<StyleAttribute name="backgroundGradientType" value="linear|top|bottom" />
		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
    <StyleAttribute name='textColor' value='lightCyan'/>
  </Style>
  <Style name='Page'>
    <StyleAttribute name="backgroundColor"      value="#121219" />
		<StyleAttribute name="backgroundGradientType" value="linear|top|bottom" />
		<StyleAttribute name="backgroundGradientColor" value="#B7B7CC" />
    <StyleAttribute name='textColor' value='lightCyan'/>
  </Style>

  <Style name='*:focus'>
    <StyleAttribute name='backgroundColor'           value='#1010DD'/>
  </Style>
  <Style name='.url'>
    <StyleAttribute name='border' value='none'/>
    <StyleAttribute name='textColor' value='lightCyan'/>
    <StyleAttribute name='textDecoration' value='underline'/>
  </Style>
  <Style name='Label'>
		<StyleAttribute name='backgroundColor' value='none'/>
    <StyleAttribute name='sizePolicy'      value='dynamic'/>
    <StyleAttribute name='textColor'       value='lightCyan'/>
  </Style>
  <Style name='DateEdit.parttime'>
    <StyleAttribute name='buttonIcon' value='smiley'/>
    <StyleAttribute name='daysOff' value='saturday sunday monday'/>
    <StyleAttribute name='firstDayOfWeek' value='tuesday'/>
  </Style>
  <Style name='ProgressBar'>
    <StyleAttribute name='percentageVisible' value='yes'/>
  </Style>
  <Style name='ComboBox'>
    <StyleAttribute name='backgroundColor' value='window'/>
  </Style>
  <Style name='Edit'>
    <StyleAttribute name='backgroundColor' value='window'/>
  </Style>
  <Style name='TextEdit'>
    <StyleAttribute name='backgroundColor' value='window'/>
  </Style>
  <Style name='TimeEdit'>
    <StyleAttribute name='backgroundColor' value='window'/>
  </Style>
  <Style name='SpinEdit'>
    <StyleAttribute name='backgroundColor' value='window'/>
  </Style>
  <Style name='Edit.noentry'>
    <StyleAttribute name='textColor' value='darkred'/>
